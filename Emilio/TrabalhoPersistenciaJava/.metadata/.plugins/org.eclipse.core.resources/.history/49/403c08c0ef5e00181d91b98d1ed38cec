package br.com.fiap.entidade;


public class AlunoCursoNota {
	private int id; 
	private int nota; 
	private AlunoCurso alunoCurso;

	
	 
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public AlunoCurso getAluno() {
		return AlunoCurso;
	}
	public void setAluno(AlunoCurso AlunoCurso) {
		this.alunoCurso = AlunoCurso;
	}
	public Curso getCurso() {
		return curso;
	}
	public void setCurso(Curso curso) {
		this.curso = curso;
	}
	public int getNota() {
		return nota;
	}
	public void setNota(int nota) {
		this.nota = nota;
	} 
	
}
