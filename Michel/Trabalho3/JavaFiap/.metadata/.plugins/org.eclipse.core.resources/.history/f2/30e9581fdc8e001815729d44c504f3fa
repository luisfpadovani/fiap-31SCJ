package br.com.fiap;

public class Pessoa interface Comparable{
	
	String nome;
	int idade;
	char sexo;
	String empresa;
	
	
	public int compareTo(Pessoa outraPessoa) {
        if (this.nome < outraPessoa.nome) {
            return -1;
        }
        if (this.nome > outraPessoa.nome) {
            return 1;
        }
        return 0;
    }
}
