package br.com.fiap.linq;

import java.time.LocalDate;
import java.time.LocalDateTime;
import java.time.Month;
import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		PreencherClientes();

	}
	
	private static void PreencherClientes() { 
		ArrayList<Cliente> listaCliente = new ArrayList<>();
		Cliente cliente1 =  new Cliente("Cliente 1",30,true, LocalDate.of(2018, Month.JULY, 1));
		Cliente cliente2 =  new Cliente("Cliente 2",31,true, LocalDate.of(2018, Month.JULY, 2));
		Cliente cliente3 =  new Cliente("Cliente 3",32,true, LocalDate.of(2018, Month.JULY, 3));
		Cliente cliente4 =  new Cliente("Cliente 4",33,true, LocalDate.of(2018, Month.JULY, 4));
		Cliente cliente5 =  new Cliente("Cliente 5",34,true, LocalDate.of(2018, Month.JULY, 5));
		listaCliente.add(cliente1);
		listaCliente.add(cliente2);
		listaCliente.add(cliente3);
		listaCliente.add(cliente4);
	}

}
